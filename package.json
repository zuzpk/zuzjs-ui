{
  "name": "@zuzjs/ui",
  "version": "0.9.0",
  "keywords": [
    "react",
    "zuz",
    "zuz.js",
    "zuz ui",
    "zuzui"
  ],
  "description": "ZuzJS UI Library",
  "author": "Zuz.js Team <support@zuz.com.pk>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "zuzjs": "dist/cjs/bin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./styles": {
      "import": "./dist/css/styles.css",
      "require": "./dist/css/styles.css"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:css": "sass --watch --no-source-map --silence-deprecation=legacy-js-api src/scss:dist/css --style compressed",
    "dev:cjs": "tsc -d -w -p tsconfig.json",
    "dev:esm": "tsc -d -w -p tsconfig.esm.json",
    "dev": "npm-run-all -p dev:*",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "dependencies": {
    "@types/md5": "^2.3.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@zuzjs/core": "^0.2.0",
    "chokidar": "^3.6.0",
    "commander": "^13.1.0",
    "hashids": "^2.3.0",
    "js-cookie": "^3.0.5",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "nanoid": "^5.0.9",
    "picocolors": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.84.0"
  },
  "devDependencies": {
    "node-loader": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}