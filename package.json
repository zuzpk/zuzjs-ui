{
  "name": "@zuzjs/ui",
  "version": "0.7.4",
  "keywords": [
    "react",
    "zuz",
    "zuz.js",
    "zuz ui",
    "zuzui"
  ],
  "description": "ZuzJS UI Library",
  "author": "Zuz.js Team <support@zuz.com.pk>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
