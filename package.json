    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "nanoid": "^5.0.9",
    "picocolors": "^1.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.84.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "node-loader": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
